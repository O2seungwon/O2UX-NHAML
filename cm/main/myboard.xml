 * @description 버튼 클릭 시, 마이보드 숨김/펼침
 * @hidden N
 * @exception 
 */
scwin.grp_boardBtn_onclick = function (e) {
    if (grp_myboardMain.hasClass("open-active")) {
        grp_myboardMain.removeClass("open-active");
    } else {
        grp_myboardMain.addClass("open-active");
    }
};
